<h2>My Movies</h2>
<div class="table-container">    
    <table align="center">
        <thead>
            <tr>
                <th>Title</th>
                <th>Year</th>
                <th>Director</th>
            </tr>
        </thead>
        <tbody>
            {{#each list}}
                <tr>
                    <td>{{this.title}}</td>
                    <td>{{this.year}}</td>
                    <td>{{this.director}}</td>
                </tr>
            {{/each}}
        </tbody>
    </table>
</div>

<h3>
    <a href="/movies">All Movies</a>
    &nbsp;&nbsp;&nbsp;
    <a href="/mymovies">My Movie</a>
</h3>